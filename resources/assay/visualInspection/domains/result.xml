<ap:domain xmlns:exp="http://cpas.fhcrc.org/exp/xml" xmlns:ap="http://labkey.org/study/assay/xml">
    <exp:Description>Visual results</exp:Description>
    <exp:PropertyDescriptor>
        <exp:Name>Timepoint</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Time Point</exp:Label>
        <exp:FK>
            <exp:Schema>lists</exp:Schema>
            <exp:Query>Timepoints</exp:Query>
        </exp:FK>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Temperature</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#int</exp:RangeURI>
        <exp:Label>Temperature</exp:Label>
        <exp:FK>
            <exp:Schema>lists</exp:Schema>
            <exp:Query>Temperatures</exp:Query>
        </exp:FK>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Pass</exp:Name>
        <exp:Required>true</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#boolean</exp:RangeURI>
        <exp:Label>Pass</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Color</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Color</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Opacity</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Opacity</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Phase</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Phase</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>Comment</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#string</exp:RangeURI>
        <exp:Label>Comment</exp:Label>
    </exp:PropertyDescriptor>
    <exp:PropertyDescriptor>
        <exp:Name>CreatedTime</exp:Name>
        <exp:Required>false</exp:Required>
        <exp:RangeURI>http://www.w3.org/2001/XMLSchema#dateTime</exp:RangeURI>
        <exp:Label>Created</exp:Label>
    </exp:PropertyDescriptor>
</ap:domain>